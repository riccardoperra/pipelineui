{
  "compilerOptions": {
    "target": "ESNext",
    "baseUrl": "./",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "types": ["vinxi/types/client"],
    "isolatedModules": true,
    "paths": {
      "~/*": ["./src/*"],
      "#editor-layout/*": ["src/components/Editor/Layout/*"],
      "#ui/*": ["src/ui/*"],
      "#editor-store/*": ["src/components/Editor/store/*"]
    }
  }
}
